{
	"name": "SimpleFrameworks",
	"version": "0.1.0",
	"private": true,
	"description": "SimpleFrameworks is a framework for develop Api with NodeJS",
	"main": "build/app/start_server.js",
	"scripts": {
		"start": "npm run env:dev -dev",
		"env:dev": "ts-node-dev --respawn --transpileOnly --debug server/app/start_server.ts -dev",
		"env:prod": "node build/app/start_server.js -prod",
		"build": "tsc",
		"version": "auto-changelog --output server/doc/changelog/CHANGELOG.md",
		"clean": "npm ci",
		"rmdir": "rm -rf node_modules"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Eito33/SimpleFrameworks.git"
	},
	"dependencies": {
		"bcrypt": "^3.0.6",
		"body-parser": "^1.18.3",
		"chalk": "^2.4.2",
		"clear": "^0.1.0",
		"cors": "^2.8.5",
		"express": "^4.16.4",
		"figlet": "^1.2.1",
		"inquirer": "^6.3.1",
		"mysql": "^2.16.0",
		"node-pre-gyp": "^0.12.0",
		"ip": "^1.1.5"
	},
	"devDependencies": {
		"@types/express": "^4.17.0",
		"@types/node": "^12.0.8",
		"morgan": "^1.9.1",
		"nodemon": "^1.19.1",
		"ts-node-dev": "^1.0.0-pre.40",
		"typescript": "^3.5.2",
		"auto-changelog": "^1.13.0"
	},
	"keywords": [],
	"author": "Gabin Rimbault",
	"license": "Apache 2.0"
}
